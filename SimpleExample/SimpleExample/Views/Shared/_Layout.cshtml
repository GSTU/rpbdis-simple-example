<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link class="logo" rel="icon" href="~/Content/themes/base/images/logo.jpg" sizes="16x16" type="image/png">
    <link rel="stylesheet" href="~/Content/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/jquery.dataTables.min.css" type="text/css" />
    @Styles.Render("~/Content/css")

    @Scripts.Render("~/Scripts/jquery-2.1.3.min.js")
    @Scripts.Render("~/Scripts/jquery-2.1.3.js")
    @Scripts.Render("~/Scripts/jquery.dataTables.min.js")

    @Scripts.Render("~/Scripts/bootstrap.min.js")
    @Scripts.Render("~/Scripts/bootstrap.js")
</head>
<body>
    <header>
        <div id="titl">
            <a href="~/Home"><img class="logo" src="~/Content/themes/base/images/logo.jpg" height="80" width="80" /></a>
            &nbsp;&nbsp;&nbsp; Simple Example
        </div>
    </header>
    <nav>
        <a class="navButton" href="~/Home">Home</a>
        <a class="navButton" href="~/Home/Users">Users</a>
        <a class="navButton" href="~/Home/UserRoles">UserRoles</a>
        <a class="navButton" href="~/Home/Roles">Roles</a>
    </nav>
    <section>
        @RenderBody()
        <!--Scripts.Render("~/bundles/jquery")-->
        @RenderSection("scripts", required: false)
    </section>
    <footer>
        &copy; Created for GSTU named after P.Suhoi
    </footer>
</body>
</html>
